<?php

/**
 * @file
 * Contains earth_api module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function earth_api_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.earth_api':
      return '<p>' . t('Provides API endpoints with exact parity to the original Mantle API.') . '</p>';
  }
}